<div class = "place-content">
  <h3> <%= @place.title %> </h3>
  <%= @place.content.html_safe %>
  <%= link_to "Destroy", place_path(@place), method: :delete, class: "btn btn-default" %>
  <button type="button" class="btn btn-default" data-action="place_edit" data-url="<%=edit_place_path(@place)%>">Edit</button>
</div>